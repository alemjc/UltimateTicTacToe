<resources>

    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">-->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:colorPrimaryDark">@color/darkeryellow</item>

    </style>

    <style name="myactionbarTheme" parent="@style/ThemeOverlay.AppCompat.ActionBar">
    </style>

    <style name="myactionbarPopupTheme" parent="@style/ThemeOverlay.AppCompat.Light">
    </style>


    <style name="rulesBodyText">
        <item name="android:textAppearance">@android:style/TextAppearance.Material.Body2</item>
        <item name="android:paddingLeft">65dp</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <style name="rulesTitleText">
        <item name="android:textAppearance">@android:style/TextAppearance.Material.Title</item>
        <item name="android:paddingLeft">45dp</item>
        <item name="android:layout_marginTop">25dp</item>
    </style>

    <style name="tile">
        <item name="android:layout_marginStart">2dp</item>
        <item name="android:layout_marginEnd">2dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="table">
        <item name="android:rowCount">3</item>
        <item name="android:columnCount">3</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>

    <style name="leftTopCornerTable" parent="table">
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="rightTopCornerTable" parent="table">
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">1dp</item>
    </style>

    <style name="bottomLeftCornerTable" parent="table">
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="bottomRightCornerTable" parent="table">
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">1dp</item>
    </style>

    <style name="middleTable" parent="table">
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="middleTopTable" parent="table">
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="middleBottomTable" parent="table">
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="middleLeftTable" parent="table">
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="middleRightTable" parent="table">
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">1dp</item>
    </style>




</resources>
